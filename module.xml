<?xml version="1.0"?>
<module xmlns="http://schema.slothsoft.net/farah/module" xmlns:sfm="http://schema.slothsoft.net/farah/module">
  <!--
Farah Module Manifest

Module: slothsoft
Vendor: slothsoft
Farah ID: farah://slothsoft@slothsoft
Composer ID: slothsoft/slothsoft

Homepage: http://farah.slothsoft.net/modules/slothsoft
Packagist: https://packagist.org/packages/slothsoft/slothsoft
GitHub: https://github.com/Faulo/slothsoft-slothsoft

Llo
-->
  <default-configuration/>
  <assets>
    <resource-directory name="data" path="data" type="application/x-php"/>
    <resource-directory name="css" path="stylesheet" type="text/css"/>
    <resource-directory name="js" path="script" type="application/javascript"/>
    <resource-directory name="xsl" path="template" type="application/xslt+xml"/>
    <resource-directory name="dictionary" path="dictionary" type="application/xml"/>
    <resource-directory name="pages" path="pages" type="application/xml"/>
    <directory name="static">
	    <resource name="heartbeat-for-all-zip" path="heartbeat-for-all/HeartbeatForAll.zip" type="application/zip"/>
	    <resource name="heartbeat-for-all-unity" path="heartbeat-for-all/Web.unity3d" type="application/vnd.unity"/>
    
      <resource-directory name="staff-content" path="staff" type="application/xml"/>
      <resource-directory name="staff-images" path="staff" type="image/png"/>
      <resource name="owner" path="owner.xml" type="application/xml"/>
      <resource name="finnish-faulo" path="finnish.faulo.xhtml" type="application/xhtml+xml"/>
      <resource name="japanese-faulo" path="japanese.faulo.xhtml" type="application/xhtml+xml"/>
      <resource name="japanese-faulo-verbs" path="japanese.faulo.verbs.xml" type="application/xml"/>
      <resource name="english-julia" path="english.julia.xhtml" type="application/xhtml+xml"/>
      <resource name="kana" path="kana.xml" type="application/xml"/>
      <resource name="vocab.kanji.map" path="vocab.kanji.map.svg" type="image/svg+xml"/>
      <resource name="pc" path="pc.xml" type="application/xml"/>
      <resource name="prefFonts" path="fonts.json" type="application/json"/>
      <resource-directory name="fonts" path="fonts" type="application/font-sfnt"/>
      <resource-directory name="icons" path="icons" type="image/png"/>
      <resource-directory name="system-fonts" path="system-fonts" type="application/font-sfnt"/>
      <resource-directory name="language-images" path="lang" type="image/svg+xml"/>
      <resource name="lang-de-de" path="lang/de-de.svg" type="image/svg+xml"/>
      <resource name="lang-en-us" path="lang/en-us.svg" type="image/svg+xml"/>
      <resource name="lang-ja-jp" path="lang/ja-jp.svg" type="image/svg+xml"/>
      <resource-directory name="lang" path="lang" type="image/svg+xml"/>
      <resource name="fire-emblem" path="fire-emblem.xml" type="application/xml"/>
      <resource-directory name="fire-emblem-data" path="fire-emblem" type="text/csv">
        <options del="|" sep="" output="table" min-length="1"/>
      </resource-directory>
      <resource-directory name="fire-emblem-por" path="fire-emblem-por" type="text/csv">
        <options del="|" sep="" output="table" min-length="1"/>
      </resource-directory>
      <resource-directory name="fire-emblem-sd" path="fire-emblem-sd" type="text/csv">
        <options del="|" sep="" output="table" min-length="1"/>
      </resource-directory>
      <resource-directory name="fire-emblem-bs" path="fire-emblem-bs" type="text/csv">
        <options del="|" sep="" output="table" min-length="1"/>
      </resource-directory>
      <resource-directory name="fire-emblem-ss" path="fire-emblem-ss" type="text/csv">
        <options del="|" sep="" output="table" min-length="1"/>
      </resource-directory>
      <resource-directory name="fire-emblem-aw" path="fire-emblem-aw" type="text/csv">
        <options del="|" sep="" output="table" min-length="1"/>
      </resource-directory>
      <resource-directory name="fire-emblem-rd" path="fire-emblem-rd" type="text/csv">
        <options del="|" sep="" output="table" min-length="1"/>
      </resource-directory>
      <resource name="fire-emblem-shadow-dragon" path="FireEmblemShadowDragon.csv" type="text/csv">
        <options del="|" sep="" cols="13" output="table"/>
      </resource>
      <resource name="fire-emblem-shadow-dragon2" path="FireEmblemShadowDragon2.csv" type="text/csv">
        <options del="|" sep="" cols="9" output="table"/>
      </resource>
      <resource name="fire-emblem-path-of-radiance" path="FireEmblemPathOfRadiance.csv" type="text/csv">
        <options del="|" sep="" cols="9" output="table"/>
      </resource>
      <resource name="fire-emblem-path-of-radiance2" path="FireEmblemPathOfRadiance2.txt" type="text/plain"/>
      <resource name="unicode-mapper" path="unicode-mapper.xml" type="application/xml"/>
      <resource name="unicode-mapping" path="unicode-mapping.xml" type="application/xml"/>
      <resource-directory name="pics" path="pics" type="image/png"/>
      <resource name="favicon" path="favicon.png" type="image/png"/>
      <resource-directory name="games" path="games" type="application/zip"/>
      <resource-directory name="unity" path="games" type="application/vnd.unity"/>
      <resource-directory name="music-mp3" path="music-mp3" type="audio/mpeg"/>
      <resource-directory name="music-lyrics" path="music-lyrics" type="text/plain"/>
      <resource-directory name="vocab-ja" path="vocab-ja" type="audio/mpeg"/>
      <resource-directory name="vocab-jlpt" path="vocab-jlpt" type="application/xhtml+xml"/>
      <resource name="grammar-ja" path="japanese.grammar.xml" type="application/xml"/>
    </directory>
    <fragment name="empty">
      <include-fragment ref="/default"/>
    </fragment>
    <fragment name="bahn">
      <param name="standalone" scope="global"/>
      <use-template ref="xsl/_bahn"/>
      <use-document ref="data/bahn"/>
    </fragment>
    <fragment name="slothsoft-chat">
      <param name="chat-database" value="slothsoft"/>
      <use-template ref="//chat/xsl/shoutbox"/>
      <use-document ref="//chat/default" as="chat"/>
    </fragment>
    <fragment name="missing-sloth-chat">
      <param name="chat-database" value="missingsloth"/>
      <use-template ref="xsl//chat/shoutbox"/>
      <use-document ref="//chat/default" as="chat"/>
    </fragment>
    <fragment name="archive-chat">
      <param name="chat-database" value="archive"/>
      <use-template ref="xsl//chat/shoutbox"/>
      <use-document ref="//chat/default" as="chat"/>
    </fragment>
    <fragment name="home">
      <include-fragment ref="default"/>
      <fragment name="content">
        <use-template ref="xsl/home"/>
      </fragment>
      <use-document ref="slothsoft-chat" as="chat"/>
    </fragment>
    <fragment name="about-us">
      <include-fragment ref="default"/>
      <fragment name="content">
        <use-template ref="xsl/about-us"/>
        <use-document ref="//farah/request"/>
        <use-document ref="static/staff-content"/>
      </fragment>
    </fragment>
    <fragment name="missingSloth">
      <include-fragment ref="/default"/>
      <use-document ref="//extensions/home-missingSloth" as="content"/>
      <use-document ref="/missing-sloth-chat" as="chat"/>
    </fragment>
    <fragment name="chat-list">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <use-template ref="xsl/chat-list"/>
        <use-document ref="slothsoft-chat" as="chat"/>
        <use-document ref="//minecraft/chat" as="chat"/>
        <use-document ref="//amber/chat" as="chat"/>
        <use-document ref="//talesof/chat" as="chat"/>
        <use-document ref="fire-emblem-chat" as="chat"/>
      </fragment>
    </fragment>
    <fragment name="imprint">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <use-template ref="xsl/address"/>
        <use-document ref="static/owner"/>
      </fragment>
    </fragment>
    <fragment name="navi">
      <use-template ref="xsl/navi"/>
      <use-document ref="//farah/sites"/>
    </fragment>
    <fragment name="pc">
      <param name="standalone" scope="global"/>
      <use-template ref="xsl/_pc"/>
      <use-document ref="data/pc"/>
      <use-stylesheet ref="css/default"/>
      <use-stylesheet ref="css/fonts"/>
      <use-script ref="js//core/html5"/>
    </fragment>
    <fragment name="validator">
      <use-template ref="xsl/w3c.validator"/>
    </fragment>
    <fragment name="dict.select">
      <use-template ref="xsl/dict.select"/>
      <use-document ref="static/language-images"/>
    </fragment>
    <fragment name="downloads">
      <use-template ref="xsl/_downloads"/>
      <use-stylesheet ref="css/default"/>
      <use-stylesheet ref="css/layout"/>
      <use-stylesheet ref="css/fonts"/>
      <use-document ref="data/downloads"/>
    </fragment>
    <fragment name="archive">
      <use-template ref="xsl/_archive"/>
      <use-document ref="data/archive"/>
      <use-stylesheet ref="css/default"/>
      <use-stylesheet ref="css/layout"/>
      <use-stylesheet ref="css/fonts"/>
      <use-stylesheet ref="css/colors"/>
      <use-stylesheet ref="css/archive"/>
      <include-fragment ref="//sse/script"/>
      <use-script ref="js/Unison"/>
      <use-document ref="/archive-chat" as="chat"/>
      <use-script ref="js//core/HTML5"/>
    </fragment>
    <fragment name="archive-hentai">
      <use-template ref="xsl/_archive.hentai"/>
      <use-document ref="data/archive.hentai"/>
      <use-stylesheet ref="css/default"/>
      <use-stylesheet ref="css/layout"/>
      <use-stylesheet ref="css/fonts"/>
      <use-stylesheet ref="css/colors"/>
      <use-stylesheet ref="css/archive"/>
    </fragment>
    <fragment name="archive-manga">
      <use-template ref="xsl/_archive.hentai"/>
      <use-document ref="data/archive.manga"/>
      <use-stylesheet ref="css/default"/>
      <use-stylesheet ref="css/layout"/>
      <use-stylesheet ref="css/fonts"/>
      <use-stylesheet ref="css/colors"/>
      <use-stylesheet ref="css/archive"/>
    </fragment>
    <fragment name="unicode-mapper">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <include-fragment ref="//core/DOM"/>
        <use-script ref="js/UnicodeMapper"/>
        <use-template ref="xsl/unicode-mapper"/>
      </fragment>
    </fragment>
    <fragment name="kana-home">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <use-document ref="//farah/sites"/>
        <use-template ref="xsl/kana-home"/>
      </fragment>
    </fragment>
    <fragment name="kana-table">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <use-template ref="xsl/kana"/>
        <use-stylesheet ref="css/kana"/>
        <use-document ref="kana"/>
      </fragment>
    </fragment>
    <fragment name="kana-translator">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <use-template ref="xsl/kana-translator"/>
        <use-stylesheet ref="css/kana"/>
        <include-fragment ref="//core/DOM"/>
        <use-script ref="js/Translator"/>
        <use-document ref="kana"/>
      </fragment>
    </fragment>
    <fragment name="kana-test">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <use-template ref="xsl/kana-test"/>
        <use-document ref="data/kana.test"/>
        <use-stylesheet ref="css/kana"/>
        <include-fragment ref="//core/DOM"/>
      </fragment>
    </fragment>
    <fragment name="kana-vocab">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <use-template ref="xsl/kana-vocab"/>
        <use-document ref="data/kana.vocab"/>
        <use-stylesheet ref="css/kana"/>
        <include-fragment ref="//core/DOM"/>
        <use-script ref="js/Translator"/>
        <use-script ref="js/Vocab"/>
        <use-script ref="js/VocabTest"/>
      </fragment>
    </fragment>
    <fragment name="kana-vocab-mobile">
      <use-template ref="xsl/_default"/>
      <use-document ref="//farah/sites"/>
      <use-stylesheet ref="css/default"/>
      <use-stylesheet ref="css/colors"/>
      <use-stylesheet ref="css/layout"/>
      <fragment name="content">
        <use-template ref="xsl/kana-vocab"/>
        <use-document ref="data/kana.vocab"/>
        <use-stylesheet ref="css/kana"/>
        <use-stylesheet ref="css/kana-mobile"/>
        <include-fragment ref="//core/DOM"/>
        <use-script ref="js/Translator"/>
        <use-script ref="js/Vocab"/>
        <use-script ref="js/VocabTest"/>
      </fragment>
    </fragment>
    <fragment name="vocab-words-log">
      <use-template ref="xsl/vocab-log"/>
