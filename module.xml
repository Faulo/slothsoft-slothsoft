        <use-template ref="xsl/vocab-list-add"/>
        <use-document ref="data/kana.vocab"/>
        <use-stylesheet ref="css/kana"/>
        <include-fragment ref="//core/DOM"/>
        <use-script ref="js/Translator"/>
      </fragment>
    </fragment>
    <fragment name="vocab-kanji">
      <include-fragment ref="/default"/>
      <fragment name="content">
        <use-template ref="xsl/vocab-kanji"/>
        <use-document ref="data/vocab.kanji"/>
        <use-stylesheet ref="css/kana"/>
        <include-fragment ref="//core/DOM"/>
        <use-script ref="js/Translator"/>
        <use-script ref="js/Vocab"/>
        <use-script ref="js/VocabTest"/>
      </fragment>
    </fragment>
    <fragment name="vocab-kanji-mobile">
      <use-template ref="xsl/_default"/>
