<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:html="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"	xmlns:sfm="http://schema.slothsoft.net/farah/module"	xmlns:sfs="http://schema.slothsoft.net/farah/sitemap">		<xsl:import href="farah://slothsoft@farah/xsl/module"/> 		<xsl:template match="/*">		<xsl:variable name="pageNode" select="*[@name = 'sites']//*[@current]"/>		<xsl:variable name="pageNodeList" select="$pageNode/ancestor-or-self::sfs:*"/>		<xsl:variable name="chatNode" select="*[@name='chat']"/>				<xsl:text disable-output-escaping='yes'>&lt;!DOCTYPE html&gt;</xsl:text>		 		<html>			<head>				<title>					<!--<xsl:value-of select="page/@header"/>-->					<xsl:for-each select="$pageNodeList">						<xsl:sort select="position()" order="descending"/>						<xsl:if test="position() &gt; 1">							<xsl:text> - </xsl:text>						</xsl:if>						<p data-dict="."><xsl:value-of select="@name"/></p>					</xsl:for-each>				</title>				<link rel="icon" type="image/png" href="/getAsset.php/slothsoft/static/favicon"/>								<link rel="canonical" href="{$pageNode/@url}"/>				<link rel="alternate" href="{$pageNode/@url}" hreflang="x-default" />				<link rel="alternate" href="{$pageNode/@url}?lang=de-de" hreflang="de-de" />				<link rel="alternate" href="{$pageNode/@url}?lang=en-us" hreflang="en-us" />						<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=yes"/>				<meta name="author" content="Daniel Schulz"/>				<meta name="keywords">					<xsl:attribute name="content">						<xsl:for-each select="$pageNodeList">							<xsl:if test="position() &gt; 1">								<xsl:text>,</xsl:text>							</xsl:if>							<p data-dict="."><xsl:value-of select="@name"/></p>						</xsl:for-each>					</xsl:attribute>				</meta>			</head>			<body class="layout-slothsoft">				<div>					<div data-media="screen"/>					<header class="Title">						<xsl:copy-of select="*[@name='status']/node()"/>						<h1 data-media="screen">							<xsl:if test="count($pageNodeList) &gt; 1">								<small class="breadcrumb">									<xsl:for-each select="$pageNodeList">										<xsl:variable name="pos" select="position()"/>										<xsl:if test="position() &gt; 1">											<xsl:text> » </xsl:text>										</xsl:if>										<xsl:choose>											<xsl:when test="position() = last()"/>											<xsl:when test="not(@ref)">												<span data-dict=""><xsl:value-of select="@name"/></span>											</xsl:when>											<xsl:otherwise>												<span data-dict=""><xsl:value-of select="@name"/></span>												<!--												<a href="{@uri}" data-dict="">													<xsl:value-of select="@name"/>												</a>												-->											</xsl:otherwise>										</xsl:choose>									</xsl:for-each>								</small>							</xsl:if>							<span data-dict=""><xsl:value-of select="$pageNodeList[last()]/@name"/></span>						</h1>						<details data-media="handheld">							<summary>								<xsl:for-each select="$pageNodeList">									<xsl:variable name="pos" select="position()"/>									<xsl:if test="position() &gt; 1">										<xsl:text> » </xsl:text>									</xsl:if>									<span data-dict=""><xsl:value-of select="@name"/></span>								</xsl:for-each>							</summary>							<xsl:copy-of select="*[@name='navi']/node()"/>						</details>					</header>				</div>				<div>					<nav data-media="screen">						<xsl:copy-of select="*[@name='navi']/node()"/>						<xsl:copy-of select="*[@name='dict.select']/node()"/>						<aside>							<xsl:copy-of select="*[@name='validator']/node()"/>						</aside>					</nav>					<main class="Content">						<xsl:apply-templates select="sfm:error" mode="sfm:html"/>						<xsl:if test="$chatNode">							<aside>								<xsl:copy-of select="$chatNode/node()"/>							</aside>						</xsl:if>						<xsl:copy-of select="*[@name='content']/node()"/>					</main>				</div>			</body>		</html>			</xsl:template></xsl:stylesheet>