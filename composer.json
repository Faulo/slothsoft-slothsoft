{
    "name": "slothsoft/slothsoft",
    "description": "A Farah module containing the content of slothsoft.net.",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Daniel Schulz",
            "email": "info.slothsoft@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "slothsoft/amber": "*",
        "slothsoft/chat": "*",
        "slothsoft/core": "*",
        "slothsoft/dragonage": "*",
        "slothsoft/farah": "*",
        "slothsoft/minecraft": "*",
        "slothsoft/sse": "*",
        "slothsoft/talesof": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5",
        "slothsoft/amber": "dev-master",
        "slothsoft/chat": "dev-master",
        "slothsoft/core": "dev-master",
        "slothsoft/dragonage": "dev-master",
        "slothsoft/farah": "dev-master",
        "slothsoft/fireemblem": "dev-master",
        "slothsoft/lang": "dev-master",
        "slothsoft/minecraft": "dev-master",
        "slothsoft/savegame": "dev-master",
        "slothsoft/sse": "dev-master",
        "slothsoft/talesof": "dev-master",
        "slothsoft/w3c": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Slothsoft\\Slothsoft\\": "src/"
        },
        "files": [
            "src/registerModule.php"
        ]
    },
    "keywords": [
        "farah-domain",
        "farah-module",
        "slothsoft"
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": false
}
